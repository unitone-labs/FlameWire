# Copy to .env and fill in your credentials
WALLET_NAME=default
WALLET_HOTKEY=default
SUBTENSOR_NETWORK=wss://entrypoint-finney.opentensor.ai:443
# Validator-owned archive endpoint for reference truth checks.
# Falls back to SUBTENSOR_NETWORK when empty.
REFERENCE_RPC_URL=
GATEWAY_URL=https://gateway-dev.flamewire.io
VALIDATOR_API_KEY=your_validator_api_key
VALIDATOR_MAX_WORKERS=32
VALIDATOR_EMA_ALPHA=0.1
VALIDATOR_VERIFICATION_INTERVAL=480

# Logging Configuration (optional)
LOGGING_INFO=true
LOGGING_DEBUG=false
LOGGING_TRACE=false

# WandB Configuration
# Get your API key from https://wandb.ai/authorize
WANDB_API_KEY=your_wandb_api_key
